generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  name      String
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
}

model Flight {
  id          String   @id @default(uuid())
  origin      String
  destination String
  airline     String
  price       Int
  createdAt   DateTime @default(now())
}
